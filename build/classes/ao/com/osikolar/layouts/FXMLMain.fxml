   </left>
   <right>
      <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-border-color: #266E66;" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="150.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/boy-512_.png" />
               </image>
            </ImageView>
         </children>
         <opaqueInsets>
            <Insets />
